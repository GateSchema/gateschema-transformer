{
  "name": "gateschema-transformer",
  "version": "0.1.1",
  "description": "transformer for GateSchema",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tslint \"{src, test}/**/*.ts\" && tsc",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "author": "William17 <superbill2012@qq.com> https://github.com/William17",
  "repository": "https://github.com/GateSchema/gateschema-transformer",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "cz-conventional-changelog": "^2.1.0",
    "gateschema": "^0.1.0",
    "jest": "^23.5.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
